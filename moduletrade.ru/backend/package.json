{
  "name": "saas-platform-backend",
  "version": "1.0.0",
  "description": "ModuleTrade SaaS Platform Backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:dev": "nodemon src/server.js",
    "start:prod": "NODE_ENV=production node src/server.js",
    "migrate": "node scripts/safe_migrate.js",
    "migrate:force": "FORCE_UNSAFE_MIGRATIONS=true node scripts/safe_migrate.js",
    "migrate:check": "node scripts/safe_migrate.js --dry-run",
    "seed": "node scripts/seed.js",
    "test": "jest",
    "lint": "eslint src/",
    "db:backup": "pg_dump -h ${DB_HOST:-postgres} -U ${DB_USER:-postgres} -d ${DB_NAME:-saas_platform} > backup_$(date +%Y%m%d_%H%M%S).sql",
    "db:status": "node scripts/migration_status.js",
    "create:user": "node scripts/createUser.js"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "stripe": "^14.5.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "saas",
    "moduletrade",
    "marketplace",
    "inventory",
    "multitenancy"
  ],
  "author": "Way7Creation",
  "license": "UNLICENSED"
}